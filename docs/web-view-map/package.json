{
  "name": "web-view-map",
  "version": "1.0.0",
  "description": "没有使用webpack-server，使用webpack-server导致无法加载\r 使用https://www.npmjs.com/package/concurrently 同时运行webpack和light-server",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "light-server -s .",
    "start": "webpack --watch --progress --devtool=inline-source-map",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run serve\"",
    "build": "webpack --optimize-minimize --env=production"
  },
  "author": "Ka",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-weui": "^1.1.3",
    "style-loader": "^0.20.3",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "weui": "^1.1.2",
    "concurrently": "^3.5.1",
    "light-server": "^2.5.0"
  }
}
